{
    "name": "csa/guzzle-bundle",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "description": "A bundle integrating GuzzleHttp 5 for Symfony 3 and compatible with PHP 7.1+",
    "authors": [
        {
            "name": "Charles Sarrazin",
            "email": "charles@sarraz.in"
        },
        {
        "name": "MickaÃ«l Andrieu",
        "email": "mickael.andrieu@prestashop.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^5.0",
        "guzzlehttp/log-subscriber": "^1.0",
        "symfony/framework-bundle": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/http-kernel": "^3.4",
        "twig/twig": "^2.6.2",
        "symfony/expression-language": "^3.4"
    },
    "suggest": {
        "doctrine/cache": "Allows caching of responses",
        "guzzlehttp/guzzle-services": "Allow web service description"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "doctrine/cache": "^1.1",
        "symfony/web-profiler-bundle": "^3.4",
        "guzzlehttp/guzzle-services": ">=0.3|<0.6",
        "symfony/yaml": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Csa\\Bundle\\GuzzleBundle\\": "src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
